e.json())\n            .then((data) => {\n                console.log(data)\n                sessionStorage.setItem('username',data.login)\n                this.setState({username:data.login})\n            })\n        }\n    }\n    \n}\n\n\nexport default withRouter(Header);","/Users/aakash/Desktop/api/aryabhata-course-module/hotelapp/src/Component/Footer.js",[],"/Users/aakash/Desktop/api/aryabhata-course-module/hotelapp/src/Component/Home/Home.js",[],"/Users/aakash/Desktop/api/aryabhata-course-module/hotelapp/src/Component/Home/Search.js",[],"/Users/aakash/Desktop/api/aryabhata-course-module/hotelapp/src/Component/Home/QuickSearch.js",[],"/Users/aakash/Desktop/api/aryabhata-course-module/hotelapp/src/Component/Home/QuickDisplay.js",["79"],"import React from 'react';\nimport './QuickSearch.css';\nimport {Link} from 'react-router-dom';\nimport myObj from './abc';\n\n\nconst QuickSearch = (props) => {\n\n    console.log(myObj.users)\n    const listTrip = ({tripdata}) => {\n        if(tripdata){\n            return tripdata.map((item) => {\n                return(\n                    <Link to={`/list/${item.trip}`}>\n                        <div className=\"tileContainer\">\n                            <div className=\"tileComponent1\">\n                                <img src={item.image}/>\n                            </div>\n                            <div className=\"tileComponent2\">\n                                <div className=\"componentHeading\">\n                                    {item.name}\n                                </div>\n                                <div className=\"componentSubHeading\">\n                                    Start your {item.name} trip with us.\n                                </div>\n                            </div>\n                        </div>\n                    </Link>\n                )\n            })\n        }\n    }\n\n    return(\n        <div>\n            <div className=\"quickSearchContainer\">\n       